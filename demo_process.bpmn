<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:zeebe="http://camunda.org/schema/zeebe/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="definitions_demo-process-5" targetNamespace="http://bpmn.io/schema/bpmn" exporter="BPM DSL" exporterVersion="1.0">
  <process id="demo-process-5" name="Demo Process 5" isExecutable="true">
    <startEvent id="start-demo" name="Start Demo"/>
    <scriptTask id="process-data" name="Process Data">
      <extensionElements>
        <zeebe:script expression="=localUserData" resultVariable="statusResult"/>
        <zeebe:ioMapping>
          <zeebe:input source="=userData" target="localUserData"/>
          <zeebe:output source="=processedData" target="processedData"/>
          <zeebe:output source="=statusResult" target="status"/>
        </zeebe:ioMapping>
      </extensionElements>
    </scriptTask>
    <exclusiveGateway id="check-status" name="Check Status"/>
    <endEvent id="success" name="Success"/>
    <endEvent id="failure" name="Failure"/>
    <sequenceFlow id="flow_start-demo_to_process-data" sourceRef="start-demo" targetRef="process-data"/>
    <sequenceFlow id="flow_process-data_to_check-status" sourceRef="process-data" targetRef="check-status"/>
    <sequenceFlow id="flow_check-status_to_success" sourceRef="check-status" targetRef="success">
      <conditionExpression xsi:type="tFormalExpression">=status = 3</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow_check-status_to_failure" sourceRef="check-status" targetRef="failure">
      <conditionExpression xsi:type="tFormalExpression">=status != 3</conditionExpression>
    </sequenceFlow>
  </process>
  <bpmndi:BPMNDiagram id="diagram_demo-process-5">
    <bpmndi:BPMNPlane id="plane_demo-process-5" bpmnElement="demo-process-5">
      <bpmndi:BPMNShape id="shape_start-demo" bpmnElement="start-demo">
        <dc:Bounds x="50" y="150" width="36" height="36"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="shape_process-data" bpmnElement="process-data">
        <dc:Bounds x="286" y="150" width="100" height="80"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="shape_check-status" bpmnElement="check-status">
        <dc:Bounds x="586" y="150" width="50" height="50"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="shape_success" bpmnElement="success">
        <dc:Bounds x="836" y="97" width="36" height="36"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="shape_failure" bpmnElement="failure">
        <dc:Bounds x="836" y="217" width="36" height="36"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="edge_flow_start-demo_to_process-data" bpmnElement="flow_start-demo_to_process-data">
        <di:waypoint x="86" y="168"/>
        <di:waypoint x="186" y="168"/>
        <di:waypoint x="186" y="190"/>
        <di:waypoint x="286" y="190"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="edge_flow_process-data_to_check-status" bpmnElement="flow_process-data_to_check-status">
        <di:waypoint x="386" y="190"/>
        <di:waypoint x="486" y="190"/>
        <di:waypoint x="486" y="175"/>
        <di:waypoint x="586" y="175"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="edge_flow_check-status_to_success" bpmnElement="flow_check-status_to_success">
        <di:waypoint x="636" y="175"/>
        <di:waypoint x="736" y="175"/>
        <di:waypoint x="736" y="115"/>
        <di:waypoint x="836" y="115"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="edge_flow_check-status_to_failure" bpmnElement="flow_check-status_to_failure">
        <di:waypoint x="636" y="175"/>
        <di:waypoint x="736" y="175"/>
        <di:waypoint x="736" y="235"/>
        <di:waypoint x="836" y="235"/>
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</definitions>
